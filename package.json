{
  "name": "disaster-ninja-fe",
  "version": "2.37.0",
  "private": true,
  "homepage": "/active/",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "pnpm run i18n:import && vite",
    "cosmos": "pnpm run i18n:import && cosmos",
    "cosmos:build": "run-s i18n:import auto:cosmos:export",
    "build": "pnpm run i18n:import && vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "preupgrade:kontur": "node ./scripts/updateKonturPackages.mjs",
    "upgrade:kontur": "pnpm update $(node ./scripts/updateKonturPackages.mjs)",
    "lint-staged": "lint-staged --concurrent false",
    "test:unit": "pnpm run i18n:import && vitest",
    "coverage": "pnpm run i18n:import && vitest run --coverage",
    "lint": "npm-run-all lint:js lint:css depcruise",
    "lint:js": "eslint --quiet --ext .tsx,.ts src/",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:i18n:keys:identity": "eslint --ext .json  -c .eslintrc.identical.keys.cjs src/core/localization/translations/",
    "depcruise": "depcruise --config .dependency-cruiser.cjs src",
    "postinstall": "run-s i18n:import",
    "docker:build": "docker build -f ./Dockerfile ./ -t ghcr.io/konturio/disaster-ninja-fe:latest",
    "docker:run": "docker run --rm -d -p 80:80/tcp ghcr.io/konturio/disaster-ninja-fe:latest",
    "prepare": "husky",
    "auto:cosmos:export": "cosmos-export --config cosmos.config.json",
    "release": "node ./scripts/auto-release/index.mjs",
    "link:packages": "node ./scripts/auto-link/index.mjs",
    "i18n:export": "tsx ./scripts/i18n/convertion.ts --to-gettext",
    "i18n:import": "tsx ./scripts/i18n/convertion.ts --to-i18next",
    "i18n:report": "tsx ./scripts/i18n/diagnostic.ts",
    "i18n:update": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:gettext-sync": "sh ./scripts/i18n/gettextSync.sh"
  },
  "lint-staged": {
    "*/**/*.{ts,tsx}": [
      "eslint --cache --fix --rule 'no-console: [error, { allow: [warn, error, info, assert, debug, trace, group, groupCollapsed, groupEnd] }]'"
    ],
    "*/**/*.css": [
      "stylelint"
    ],
    "*/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*/**/en/common.json": [
      "eslint --rule 'i18n-json/valid-message-syntax: [error, { syntax: non-empty-string }]'",
      "npm run i18n:export",
      "git add *.pot"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
