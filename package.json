  "private": true,
  "homepage": "/active",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "preupgrade:k2": "node ./scripts/updatek2.mjs",
    "upgrade:k2": "npm cache clean --force && npm update $(node ./scripts/updatek2.mjs)",
