{
  "name": "disaster-ninja-fe",
  "version": "2.3.0",
  "private": true,
  "homepage": "/active/",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "npm run i18n:import && vite",
    "cosmos": "npm run i18n:import && run-p auto:cosmos:renderer auto:cosmos auto:open",
    "cosmos:build": "run-s i18n:import auto:cosmos:export auto:cosmos:renderer:export",
    "build": "npm run i18n:import && vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "preupgrade:kontur": "node ./scripts/updateKonturPackages.mjs",
    "upgrade:kontur": "npm cache clean --force && npm update $(node ./scripts/updateKonturPackages.mjs)",
    "lint-staged": "lint-staged --concurrent false",
    "test:unit": "npm run i18n:import && vitest",
    "coverage": "npm run i18n:import && vitest run --coverage",
    "lint": "npm-run-all lint:js lint:css depcruise",
    "lint:js": "eslint --quiet --ext .tsx,.ts src/",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:i18n:keys:identity": "eslint --ext .json  -c .eslintrc.identical.keys.cjs src/core/localization/translations/",
    "depcruise": "depcruise --config .dependency-cruiser.cjs src",
    "postinstall": "run-s fixtypes patch",
    "fixtypes": "node ./scripts/fix-deckgl-types.mjs",
    "patch": "patch-package",
    "docker:build": "docker build -f ./Dockerfile ./ -t ghcr.io/konturio/disaster-ninja-fe:latest",
    "docker:run": "docker run --rm -d -p 80:80/tcp ghcr.io/konturio/disaster-ninja-fe:latest",
    "prepare": "node ./scripts/husky-prepare.js",
    "auto:open": "npx open-cli http://localhost:5000",
    "auto:cosmos": "cosmos --config cosmos.config.json",
    "auto:cosmos:export": "patch-package && cosmos-export --config cosmos.config.export.json",
    "auto:cosmos:renderer": "vite --config cosmos.vite.config.ts --force serve --port=8080",
    "auto:cosmos:renderer:export": "vite build --config cosmos.vite.config.ts",
    "release": "node ./scripts/auto-release/index.mjs",
    "link:packages": "node ./scripts/auto-link/index.mjs",
    "i18n:export": "node ./scripts/i18n/convertion.mjs --to-gettext",
    "i18n:import": "node ./scripts/i18n/convertion.mjs --to-i18next",
    "i18n:update": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:gettext-sync": "sh ./scripts/i18n/gettextSync.sh"
  },
  "lint-staged": {
    "*/**/*.{ts,tsx}": [
      "eslint --cache --fix --rule 'no-console: [error, { allow: [warn, error, info, assert, debug, trace] }]'"
    ],
    "*/**/*.css": [
      "stylelint"
    ],
    "*/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*/**/en/common.json": [
      "eslint --rule 'i18n-json/valid-message-syntax: [error, { syntax: non-empty-string }]'",
      "npm run i18n:export",
      "git add *.pot"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@csstools/normalize.css": "^12.0.0",
    "@deck.gl/core": "^8.8.25",
    "@deck.gl/extensions": "^8.8.25",
    "@deck.gl/geo-layers": "^8.8.25",
    "@deck.gl/layers": "^8.8.25",
    "@deck.gl/mapbox": "^8.8.25",
    "@deck.gl/mesh-layers": "^8.8.25",
    "@konturio/default-icons": "^2.2.2",
    "@konturio/default-theme": "3.2.4",
    "@konturio/ui-kit": "^3.15.0",
    "@loaders.gl/core": "^3.3.1",
    "@mdx-js/rollup": "^2.3.0",
    "@nebula.gl/edit-modes": "1.1.0-alpha.5",
    "@nebula.gl/layers": "1.1.0-alpha.5",
    "@reatom/core": "^2.0.22",
    "@reatom/react": "^2.0.19",
    "@turf/area": "6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/centroid": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/great-circle": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/kinks": "^6.5.0",
    "@turf/union": "^6.5.0",
    "@types/file-saver": "^2.0.5",
    "@types/geojson": "^7946.0.10",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.14.0",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/w3c-generic-sensor": "^1.0.6",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "color-interpolate": "^1.0.5",
    "date-fns": "^2.29.3",
    "dnd-core": "^16.0.1",
    "file-saver": "^2.0.5",
    "geojson-polygon-self-intersections": "^1.2.1",
    "h3-js": "^3.7.2",
    "hash-wasm": "^4.9.0",
    "history": "^4.10.1",
    "hsluv": "^0.1.0",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "npm:lodash-es@4.17.21",
    "lodash-es": "4.17.21",
    "maplibre-gl": "^1.15.3",
    "nanoid": "^4.0.1",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-i18next": "^12.1.5",
    "react-lazily": "^0.9.1",
    "react-markdown": "^8.0.5",
    "react-router": "^5.3.4",
    "react-router-cache-route": "^1.12.11",
    "react-router-dom": "^5.3.4",
    "react-transition-group": "^4.4.5",
    "react-virtuoso": "^4.1.0",
    "remark-gfm": "^3.0.1",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@uiw/react-github-corners": "1.5.15",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.13",
    "axios-mock-adapter": "^1.21.2",
    "bundle-size-diff": "github:konturio/bundle-size-diff#v1.0.6",
    "cpr": "^3.0.1",
    "dependency-cruiser": "^12.9.0",
    "eslint": "^8.34.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-i18n-checker": "^1.4.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^8.4.4",
    "husky": "^8.0.3",
    "i18next-conv": "13.1.1",
    "i18next-scanner": "^4.1.1",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.1.0",
    "patch-package": "^6.5.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-loader": "^7.0.2",
    "postcss-nested": "^6.0.0",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.8.4",
    "react-cosmos": "5.7.2",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sinon": "^15.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^6.0.0",
    "ts-json-schema-generator": "^1.2.0",
    "typescript": "4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5"
  }
}
